{
  "name": "typeconnect",
  "version": "0.5.0",
  "description": "Connecting data and organizing module communication",
  "main": "index.js",
  "repository": "https://github.com/guitarino/typeconnect.git",
  "author": "Kirill Shestakov",
  "license": "MIT",
  "scripts": {
    "build": "webpack && yarn run types",
    "types": "tsc -d --emitDeclarationOnly --outDir build src/index.ts",
    "test": "ava",
    "test:cov": "c8 --include src --all yarn run test",
    "cov_report": "npm install coveralls --no-optional && c8 report --include src --all --reporter=text-lcov | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "prep": "rimraf ./build && yarn run build && cpy ./package.json ./LICENSE ./README.md ./build",
    "pub": "yarn run prep && cd build && npm publish"
  },
  "keywords": [
    "state",
    "state management",
    "derived state",
    "computed state",
    "reactive",
    "reactive programming",
    "observed",
    "computed",
    "frp"
  ],
  "dependencies": {
    "@babel/runtime": "^7.12.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/register": "^7.12.10",
    "@yarnpkg/pnpify": "^2.4.0",
    "ava": "^3.15.0",
    "babel-loader": "^8.2.2",
    "c8": "^7.4.0",
    "cpy-cli": "^3.1.1",
    "pnp-webpack-plugin": "^1.6.4",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.3",
    "webpack": "^5.12.2",
    "webpack-cli": "^4.3.1"
  }
}
