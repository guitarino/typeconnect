{
	"compilerOptions": {
		"target": "ES5",
		"experimentalDecorators": true,
		"module": "CommonJS",
		"moduleResolution": "Node",
		"allowJs": true,
		"noEmit": true,
		"strict": true,
		"isolatedModules": true,
		"esModuleInterop": true,
		"noImplicitAny": false,
		"skipLibCheck": true,
		"strictPropertyInitialization": false
	},
	"include": [
		"src",
		"test"
	]
}