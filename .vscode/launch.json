{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Debug test",
			"runtimeExecutable": "yarn",
			"runtimeArgs": [
				"ava",
				"debug",
				"--break",
				"--serial",
				"${file}"
			],
			"outputCapture": "std",
			"skipFiles": [
				"<node_internals>/**/*.js"
			]
		}
	],
	"compounds": []
}